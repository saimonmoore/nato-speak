#!/usr/bin/env node

var _ = require('lodash');

var nato = {
  'A': 'Alfa Romeo',
  'B': 'Andres',
  'C': 'Charlize',
  'D': 'Delta Force',
  'E': 'Echo & the Bunnymen',
  'F': 'Foxtrot',
  'G': 'Golfo',
  'H': 'Hotel California',
  'I': 'http://isitdownrightnow.com',
  'J': 'JotaSon',
  'K': 'Quilombo',
  'L': 'Limassol',
  'M': 'Maikel',
  'N': "No dejo, 'ta ese capullo",
  'O': 'Joscar',
  'P': 'Papito',
  'Q': 'Quebec',
  'R': 'Romero',
  'S': 'Sarah Springer',
  'T': ':trollface:',
  'U': 'Unicode',
  'V': 'Vamooosss',
  'W': 'UILaikilaikdat',
  'X': 'X-Men',
  'Y': 'YessSirIcanBoogie',
  'Z': 'Zulu Sessions'
};

var input = process.argv[2];

var output = _.map(input, function(letter) {
  return nato[letter.toUpperCase()] || letter;
}).join(' ');

console.log(output);
